#import config
. .aliasconfig

#TOMCAT
alias ctomcat='cd /usr/local/tomcat/twdrp-tomcat-8.0.30'
#bshell
alias tail='tail -fn 100'
alias grep='grep --color'
alias ..='cd ..'

alias free='free -mt'
alias valias='vim ~/.alias'
alias salias='source ~/.bash_profile'

#mysql
alias mysqlt="mysql -h${mysqladdress} -u${mysqluser} -p${mysqlpass}"
alias mysqlc='mysqlt -D '
alias mysqldump="mysqldump -h${mysqladdress} -u${mysqluser} -p${mysqlpass} --databases"
#redis
alias redis-cli='cd ${redisAddress};./redis-cli -h ${redishost}'

